{
  "collectionName": "components_containers_cubcasts",
  "info": {
    "displayName": "Cubcast",
    "icon": "users",
    "description": ""
  },
  "options": {},
  "attributes": {
    "title": {
      "type": "richtext"
    },
    "description": {
      "type": "text"
    },
    "members": {
      "type": "component",
      "repeatable": true,
      "component": "shared.media"
    },
    "trackingId": {
      "type": "string"
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "socialApps": {
      "type": "component",
      "repeatable": true,
      "component": "shared.social-links"
    },
    "verticalSpacing": {
      "type": "enumeration",
      "enum": [
        "Vertical Spacing 0%",
        "Vertical Spacing 1%",
        "Vertical Spacing 2%",
        "Vertical Spacing 3%",
        "Vertical Spacing 4%",
        "Vertical Spacing 5%",
        "Vertical Spacing 6%",
        "Vertical Spacing 7%",
        "Vertical Spacing 8%",
        "Vertical Spacing 9%",
        "Vertical Spacing 10%"
      ],
      "default": "Vertical Spacing 1%"
    }
  }
}
