{
  "collectionName": "components_containers_banners",
  "info": {
    "displayName": "Banners",
    "icon": "images",
    "description": ""
  },
  "options": {},
  "attributes": {
    "heading": {
      "type": "string"
    },
    "banners": {
      "type": "component",
      "repeatable": true,
      "component": "shared.banner"
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "trackingId": {
      "type": "string"
    },
    "verticalSpacing": {
      "type": "enumeration",
      "enum": [
        "Vertical Spacing 0%",
        "Vertical Spacing 1%",
        "Vertical Spacing 2%",
        "Vertical Spacing 3%",
        "Vertical Spacing 4%",
        "Vertical Spacing 5%",
        "Vertical Spacing 6%",
        "Vertical Spacing 7%",
        "Vertical Spacing 8%",
        "Vertical Spacing 9%",
        "Vertical Spacing 10%"
      ],
      "default": "Vertical Spacing 1%"
    }
  }
}
