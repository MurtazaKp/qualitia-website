{
  "kind": "collectionType",
  "collectionName": "integrations",
  "info": {
    "singularName": "integration",
    "pluralName": "integrations",
    "displayName": "Integration",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "title": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "media": {
      "type": "component",
      "repeatable": false,
      "component": "shared.media"
    },
    "link": {
      "type": "component",
      "repeatable": false,
      "component": "shared.cta"
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "containers.article-list",
        "containers.banners",
        "containers.book-demo",
        "containers.card-listing",
        "containers.compare-view",
        "containers.contact-form",
        "containers.cta",
        "containers.cubcast",
        "containers.embedded-scripts",
        "containers.fa-qs",
        "containers.integration-list",
        "containers.integrations-showcase",
        "containers.next-previous-feature",
        "containers.paragraph",
        "containers.resources",
        "containers.roadmap",
        "containers.security-list",
        "containers.subscribe",
        "containers.table-of-content-panel",
        "containers.tabs-list",
        "containers.teams",
        "containers.video-card"
      ]
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}
