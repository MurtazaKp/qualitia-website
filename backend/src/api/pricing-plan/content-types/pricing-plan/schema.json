{
  "kind": "collectionType",
  "collectionName": "pricing_plans",
  "info": {
    "singularName": "pricing-plan",
    "pluralName": "pricing-plans",
    "displayName": "Pricing plan",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "annualActualPrice": {
      "type": "string"
    },
    "annualSalePrice": {
      "type": "string"
    },
    "monthActualPrice": {
      "type": "string"
    },
    "monthSalePrice": {
      "type": "string"
    },
    "link": {
      "type": "component",
      "repeatable": false,
      "component": "shared.cta"
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "pricing.pricing-feature"
    },
    "tableFeatures": {
      "type": "component",
      "repeatable": true,
      "component": "pricing.table-feature-list"
    },
    "backgroundColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "trackingId": {
      "type": "string"
    },
    "priceTextMonthly": {
      "type": "string"
    },
    "priceTooltip": {
      "type": "string"
    },
    "priceTextAnnually": {
      "type": "string"
    }
  }
}
